{{#if isButtonView}}
    <button
      type="button"
      class="btn btn-small btn-darkBlue"
      on:el:click="openBulkVerifyModal(scope.element)">
      Bulk Verify
    </button>
  {{else}}
    <a
      href="javascript://"
      on:el:click="openBulkVerifyModal(scope.element)">
      <i class="fa fa-fw fa-check-square-o"></i>
      Bulk Verify
    </a>
{{/if}}
